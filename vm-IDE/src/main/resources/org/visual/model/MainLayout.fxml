<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.visual.model.mvc.controllers.LayoutController">
    <VBox fx:id="rootVBox">
        <fx:include source="GlobalMenu.fxml"/>
        <SplitPane VBox.vgrow="ALWAYS" fx:id="splitPane" dividerPositions="0.2">
            <VBox fx:id="leftDrawer" VBox.vgrow="ALWAYS">
                <TreeView fx:id="tree" VBox.vgrow="ALWAYS">
                </TreeView>
            </VBox>
            <VBox VBox.vgrow="ALWAYS">
                <Label text="Draw Canvas"/>
                <fx:include source="TextEditor.fxml"/>
            </VBox>
        </SplitPane>
    </VBox>
</StackPane>
