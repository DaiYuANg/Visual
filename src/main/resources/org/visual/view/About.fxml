<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.ButtonType?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.visual.controller.AboutController"
            prefHeight="400.0" prefWidth="600.0">
  <content>
    <VBox spacing="10" alignment="CENTER">
      <Label text="About" style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: #333;"/>
      <Label fx:id="manifestVersion" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="implementationTitle" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="implementationGroup" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="implementationVersion" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="builtBy" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="builtHost" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="builtDate" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="builtOS" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="builtJdk" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="scmRepository" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="scmBranch" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="scmCommitMessage" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="scmCommitHash" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="scmCommitAuthor" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
      <Label fx:id="scmCommitDate" style="-fx-font-size: 14px; -fx-text-fill: #555;" textAlignment="CENTER"/>
    </VBox>
  </content>
  <ButtonType text="Close" buttonData="CANCEL_CLOSE"/>
</DialogPane>
